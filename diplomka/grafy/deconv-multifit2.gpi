set terminal pdf enhanced font "Helvetica,14" linewidth 3 size 14cm,14cm
set termoption dash
set locale "cs_CZ.UTF-8"
set encoding utf8
set ylabel "{/Symbol e}_i"
set xtics 100
E(nu) = 6.626e-34*3e8*nu*1e2/1.602e-19
B(beta) = (6.626e-34*3e8*beta*1e2/1.602e-19)/(2*sqrt(2*log(2))) 
G(x,e,b) = (1/(sqrt(2*3.14)*B(b)*E(e)))*(exp(-((E(x)-E(e))**2)/(2*B(b)**2))-exp(-((E(x)+E(e))**2)/(2*B(b)**2)))

set multiplot layout 2, 2
set key font "Helvetica,11"

set tmargin 0.7
set bmargin 0.8
set rmargin 0.1
set format x ""
set format y "%.2f"

set label 1 "CH30C" at graph 0.05, graph 0.95

load "grafy/CH30Cdeconv.gpi"

set ylabel ""
set label 1 "CH30D" at graph 0.05, graph 0.95

load "grafy/CH30Ddeconv.gpi"

set bmargin 3.1
set ylabel "{/Symbol e}_i"
set xlabel "Vlnoƒçet, {/Symbol n} [cm^{-1}]"
set format x "%g"
set label 1 "CH87A" at graph 0.05, graph 0.95

load "grafy/CH87Adeconv.gpi"

set ylabel ""
set label 1 "CH89A" at graph 0.05, graph 0.95

load "grafy/CH89Adeconv.gpi"

unset multiplot
